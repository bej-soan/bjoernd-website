{
  "name": "bjoernd-website",
  "version": "0.1.0",
  "description": "Repository for my personal website",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "dev": "ELEVENTY_ENV=dev npx @11ty/eleventy --serve",
    "sass": "sass src/css:_public/css",
    "watch:sass": "npm run sass -- --watch",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "start": "npm-run-all sass --parallel watch:*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bej-soan/bjoernd-website"
  },
  "author": "bej-soan",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11tyrocks/eleventy-plugin-sass-lightningcss": "^1.0.0",
    "node-sass": "^6.0.0",
    "npm-run-all": "^4.1.5"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.3.5"
  }
}
