{
  "name": "11ty-sass-skeleton",
  "version": "3.0.0",
  "description": "Featuring absolutely nothing beyond a base HTML5 template and the essential setup to watch and compile your Sass alongside 11ty.",
  "main": "index.html",
  "scripts": {
    "build": "eleventy",
    "watch:eleventy": "npx @11ty/eleventy --serve",
    "watch:sass": "npx sass src/sass:_public/css --watch",
    "start": "npm run watch:eleventy & npm run watch:sass"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/5t3ph/11ty-sass-skeleton.git"
  },
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "@11ty/eleventy": "^2.0.0-beta.3",
    "@11tyrocks/eleventy-plugin-sass-lightningcss": "^1.0.0"
  },
  "browserslist": [
    "> 0.2%",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy-navigation": "^0.3.5"
  }
}
